QSpinBox,
QDoubleSpinBox,
QTextEdit,
.QLineEdit,
QComboBox,
QPushButton {

    color: #ff1744;
    background-color: #000a12;
    border: 1px solid #ff1744;
    border-radius: 4px;
    padding: 8px 16px ;
    height: 18px;
    font-weight: bold;
}


QSpinBox:disabled,
QDoubleSpinBox:disabled,
QTextEdit:disabled,
.QLineEdit:disabled,
QComboBox:disabled,
QPushButton:disabled
{

    color: rgba(255, 23, 68, 0.2);
    background-color: rgba(38, 50, 56, 0.75);
    border: 2px solid rgba(255, 23, 68, 0.2);
    border-width: 0 0 2px 0;
    border-radius: 4px;
    padding: 8px 16px ;
    height: 18px;
    font-weight: bold;
}




QTreeView,
QListView,
QSpinBox,
QDoubleSpinBox,
